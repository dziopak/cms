{
    "name": "dziopak/dcms",
    "type": "project",
    "description": "Yet another content management system based on Laravel framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "arrilot/laravel-widgets": "^3.13",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.0",
        "nwidart/laravel-modules": "^6.2",
        "rachidlaasri/laravel-installer": "^4.0",
        "spatie/laravel-query-builder": "^2.8",
        "tymon/jwt-auth": "dev-develop",
        "esemve/hook": "dev-master",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "repositories":[
        {
            "type": "vcs",
            "url": "git@github.com:dziopak/hook.git"
        }
    ],
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "plugins\\": "plugins/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/AppHelpers.php",
            "app/Helpers/ViewHelpers.php",
            "app/Helpers/BlockHelpers.php",
            "app/Helpers/StringHelpers.php",
            "app/Helpers/DataArrays.php",
            "app/Helpers/ArrayHelpers.php",
            "app/Helpers/DBHelpers.php",
            "app/Helpers/ThemeHelpers.php",
            "app/Helpers/JSONHelpers.php",
            "app/Helpers/DataHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
